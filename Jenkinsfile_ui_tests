pipeline{
    agent any
    triggers {
        pollSCM('*/1 * * * *')
    }
    parameters {
        String(name: 'ENVIRONMENT', defaultValue: 'DEV', description: 'Test Environment')
    }
    stages {
        stage('ui-test-execution') {
            steps {
                echo "Running UI tests"
            }
        }
    }
}